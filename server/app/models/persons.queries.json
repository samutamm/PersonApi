{
  "createTable": [
    "CREATE TABLE IF NOT EXISTS persons (",
      "id    SERIAL PRIMARY KEY,",
      "name  TEXT,",
      "email TEXT,",
      "address TEXT,",
      "username TEXT,",
      "password TEXT,",
      "role TEXT",
      " );"
  ],
  "selectByUsername": [
    "SELECT * FROM persons",
    " WHERE username = ($1)"
  ],
  "selectAll": [
    "SELECT * FROM persons;"
  ],
  "addPerson": [
    "INSERT INTO persons VALUES",
    " (DEFAULT, $1, @2, ($3), ($4), ($5), ($6) );"
  ]
}
